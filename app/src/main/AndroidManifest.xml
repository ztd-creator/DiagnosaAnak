<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.diagnosaanak">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="file.project.diagnosaanak.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/logofix"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="file.project.diagnosaanak.penyakit.p55"></activity>
        <activity android:name="file.project.diagnosaanak.penyakit.p4" />
        <activity android:name="file.project.diagnosaanak.penyakit.P3" />
        <activity
            android:name="file.project.diagnosaanak.penyakit.p1"
            android:theme="@style/AppTheme.noActionBar" />
        <activity android:name="file.project.diagnosaanak.ListPenyakit">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity android:name="file.project.diagnosaanak.ActivityMulaiDiagnosa" />
        <activity
            android:name="file.project.diagnosaanak.halamanutama"
            android:theme="@style/AppTheme.noActionBar" />
        <activity android:name="file.project.diagnosaanak.register" />
        <activity android:name="file.project.diagnosaanak.login" />
        <activity
            android:name="file.project.diagnosaanak.halamanDiagnosa"
            android:label="Sistem Pakar Anak" />
        <activity android:name="file.project.diagnosaanak.activitylogin" />
        <activity android:name="file.project.diagnosaanak.MainActivity" />
    </application>

</manifest>